
%h2 Sign up
= form_with model: @user,url: registration_path(resource_name),local: true do |f|
  = render "devise/shared/error_messages", resource: resource
  .field
    = f.label :email
    %br/
    = f.email_field :email, autofocus: true, autocomplete: "email"
  .field
    = f.label :nickname
    %br/
    = f.text_field :nickname, autofocus: true, autocomplete: "nickname"
  .field
    = f.label :name_full
    %br/
    = f.text_field :name_full, autofocus: true, autocomplete: "name_full"
  .field
    = f.label :name_cana
    %br/
    = f.text_field :name_cana, autofocus: true, autocomplete: "name_cana"
  .field
    = f.label :birth_year
    %br/
    = f.text_field :birth_year, autofocus: true, autocomplete: "birth_year"
  .field
    = f.label :birth_month
    %br/
    = f.text_field :birth_month, autofocus: true, autocomplete: "birth_month"
  .field
    = f.label :birth_day
    %br/
    = f.text_field :birth_day, autofocus: true, autocomplete: "birth_day"
  .field
    = f.label :call_number
    %br/
    = f.text_field :call_number, autofocus: true, autocomplete: "call_number"
  .field
    = f.label :encrypted_password
    %br/
    = f.password_field :encrypted_password, autofocus: true, autocomplete: "encrypted_password"

  .actions
    = f.submit "Sign up"
= render "devise/shared/links"